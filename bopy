from turtle import *
shape("turtle")
forward(100)
color("blue")
begin_fill()
circle(50)
end_fill()
left(90)
forward(50)
left(180)
begin_fill()
color("red")
forward(200)
right(90)
forward(100)
left(270)
forward(130)
end_fill()
begin_fill()
color("green")
circle(40)
end_fill()  
forward(20)
hideturtle()




exitonclick()